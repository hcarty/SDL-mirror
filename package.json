{
  "name": "@esy-ocaml/sdl2",
  "version": "2.0.9000",
  "description": "PackageJson version of SDL2.",
  "main": "index.js",
  "esy": {
    "build": [
      "./configure --prefix=#{self.lib} --libdir=#{self.lib}",
      "make"
    ],
    "install": [
      "make install"
    ],
    "exportedEnv": {
      "PKG_CONFIG_PATH": {
        "scope": "global",
        "val": "#{@esy-ocaml/sdl2.lib / 'pkgconfig' : $PKG_CONFIG_PATH}"
      }
    },
    "buildsInSource": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/esy-ocaml/SDL-mirror.git"
  },
  "keywords": [
    "esy",
    "sdl",
    "sdl2"
  ],
  "author": {},
  "license": "Zlib",
  "bugs": {
    "url": "https://github.com/esy-ocaml/SDL-mirror/issues"
  },
  "homepage": "https://github.com/esy-ocaml/SDL-mirror#readme",
  "readmeFilename": "README.txt"
}
